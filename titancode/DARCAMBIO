//
// Soluci√≥n al problema Dar Cambio
// http://www.titancod.net/problema/DARCAMBIO
//


#include <bits/stdc++.h>

#define FOR(i,x,y) for(int i=(int)x; i<(int)y; i++)
#define RSORT(a) sort(a.rbegin(), a.rend())

using namespace std;

int main(){
  vector <int> v;
  int d,m,c;
  while (scanf("%d",&d) == 1){
    FOR(i,0,d){
      cin >> m;
      v.push_back(m);
    }
    cin >> m;
    RSORT(v);
    c=0;
    FOR(i,0,d){
      while(m >= v[i]){
        c++;
        m = m-v[i];
      }
    }
    if(m==0)
      cout << c << endl;
    else
      cout << "-1" << endl;
  }
  return 0;
}
